                var an = a.name.toLowerCase(), bn = b.name.toLowerCase();
                if (an == bn)
                    return 0;
                return (an > bn) ? 1 : -1;
            });
            for (i = 0; i < list.length; i++) {
                if (list[i].id == 'paypal') {
                    var tmp = list[0];
                    list[0] = list[i];
                    list[i] = tmp;
                    break;
                }
            }
            return list;
        };
