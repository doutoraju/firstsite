		{type: 'TextField', placeholder: __('Merchant ID'), id: 'key'},
		{type: 'TextField', placeholder: __('Merchant Key'), id: 'key2'},
		{type: 'CheckBox', label: __('Test mode'), id: 'key3', css: {padding: 7, marginTop: 5, display: 'inline-block'}, init: function() {
