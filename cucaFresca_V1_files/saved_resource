                this._group.removeClass('open');
                if (currentlyOpen === this)
                    currentlyOpen = null;
            }
        };
        DropdownBox.prototype.getOpen = function () {
            return this._open;
        };
        DropdownBox.prototype.addMenuStyleClass = function (styleClass) {
            this._menu.addClass(styleClass);
        };
