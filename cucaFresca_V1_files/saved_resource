                    var touch = _a[_i];
                    if (this.dragPointerId === "unknown touch" || this.dragPointerId === touch.identifier) {
                        cursor = { x: touch.pageX, y: touch.pageY, clientX: touch.clientX, clientY: touch.clientY };
                        break;
                    }
                }
