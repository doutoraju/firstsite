    var Item = (function () {
        function Item(title, id, original, htmlTitle, icon, multiSelect, nonSelectable) {
            if (original === void 0) { original = null; }
            if (htmlTitle === void 0) { htmlTitle = ''; }
            if (icon === void 0) { icon = ''; }
