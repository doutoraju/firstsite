                case "serif":
                    fontFamily += ",Arial,serif";
                    break;
                case "monospace":
                    fontFamily += ",Arial,monospace";
                    break;
                case "handwriting":
                    fontFamily += ",Arial,cursive";
                    break;
                default:
                    fontFamily += ",Arial," + this.category;
                    break;
            }
            return fontFamily;
        };
        GoogleFont.prototype.clone = function () {
