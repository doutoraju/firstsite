                    top_2 = 0;
                }
                else if (forceBounds.top === 'bottom') {
                    top_2 = pHeight - subject.getHeight();
                }
                else if (forceBounds.top === 'middle' || forceBounds.top === 'center') {
                    top_2 = (pHeight - subject.getHeight()) / 2;
                }
            }
            else {
                top_2 = forceBounds.top;
            }
            subject.setLocation(null, top_2);
        }
        subject.onResize();
    }
