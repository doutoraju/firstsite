            buildPropertiesOpenStep(scenario, classType);
            requirement = 'timeout:500';
        }
        scenario.steps.push({
            title: ((dict.Choose && dict.Choose.title) ? dict.Choose.title : __('Choose')),
            text: ((dict.Choose && dict.Choose.text) ? dict.Choose.text : ''),
