            subject: 'item-plugin-' + classType + '-prop-apply[0]',
            objectives: ['item-plugin-' + classType + '-prop-apply']
        });
    }
    function getOffset(elem, useOverlay) {
        var pOff;
        if (useOverlay) {
            var parent_3 = elem.getParent();
            pOff = parent_3.container.offset();
            pOff.top -= 128;
        }
        else {
            pOff = { left: 0, top: 0 };
        }
        return pOff;
    }
