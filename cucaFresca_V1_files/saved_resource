                        distance = otherLeft - right;
                        if (distance <= ElementMeasurementHelpers.MAX_HORIZONTAL_DISTANCE) {
                            rightRanges.add({
                                from: Math.max(top, otherTop),
                                to: Math.min(bottom, otherBottom),
                                offset: otherLeft,
                                distance: distance
                            }, true, ElementMeasurementHelpers.checkRangePriority);
                        }
